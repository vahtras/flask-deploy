server {
    listen 443 ssl;
    location / {
        ssl_certificate: /home/www/cert.pem
        ssl_certificate_key: /home/www/key.pem
        proxy_pass https://localhost:8001;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }
    location /static {
        alias  /home/www/flask_project-staging/flask_project/static/;
    }
}
